

scnds = o_dayNightCycle.seconds
day = o_dayNightCycle.day
month = o_dayNightCycle.months
year = o_dayNightCycle.year




//DAY NAMES
if((day mod 7) == 1) {
	dayOfWeek = "MON"
} else if ((day mod 7) == 2) {
	dayOfWeek = "TUE"
} else if ((day mod 7) == 3) {
	dayOfWeek = "WED"
} else if ((day mod 7) == 4) {
	dayOfWeek = "THU"
} else if ((day mod 7) == 5) {
	dayOfWeek = "FRI"
} else if ((day mod 7) == 6) {
	dayOfWeek = "SAT"
} else if ((day mod 7) == 0) {
	dayOfWeek = "SUN"
} 




