day = o_dayNightCycle.day
month = o_dayNightCycle.months
year = o_dayNightCycle.year



//DAYS
if((day mod 7) == 1) {
	dayOfWeek = "Mon"
} else if ((day mod 7) == 2) {
	dayOfWeek = "Tue"
} else if ((day mod 7) == 3) {
	dayOfWeek = "Wed"
} else if ((day mod 7) == 4) {
	dayOfWeek = "Thu"
} else if ((day mod 7) == 5) {
	dayOfWeek = "Fri"
} else if ((day mod 7) == 6) {
	dayOfWeek = "Sat"
} else if ((day mod 7) == 7) {
	dayOfWeek = "Sun"
} 
